{
  "name": "angular-disable-animate",
  "version": "1.0.0",
  "description": "An Angular directive that disables ng-animate on selected elements.",
  "keywords": [
    "angular",
    "angular",
    "disable",
    "animation",
    "animate",
    "ngAnimate"
  ],
  "author": "Benjamin Charity <ben@benjamincharity.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjamincharity/angular-disable-animate/issues"
  },
  "homepage": "https://github.com/benjamincharity/angular-disable-animate",
  "main": [
    "dist/angular-disable-animate.js"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "WEBPACK_ENV=build webpack && osx-notifier --type \"pass\" --title \"Build successful\" --message \"gg\" --group \"npmBuild\"",
    "build:css": "npm run scss:compressed && npm run scss:expanded && npm run autoprefixer",
    "autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "scss:compressed": "node-sass --output-style compressed -o dist/ src && mv dist/angular-ratings.css dist/angular-ratings.min.css",
    "scss:expanded": "node-sass --output-style expanded -o dist/ src",
    "watch:css": "onchange 'src/*.scss' -- npm run build:css",
    "watch:js": "onchange 'src/*.js' -- npm run build:js",
    "watch": "npm run watch:js && npm run watch:css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benjamincharity/angular-disable-animate.git"
  },
  "dependencies": {
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "eslint": "1.7.2",
    "eslint-config-moment": "^1.5.0",
    "eslint-loader": "1.1.0",
    "onchange": "^2.2.0",
    "osx-notifier": "^0.2.2",
    "webpack": "1.12.9",
    "yargs": "3.32.0",
    "html-loader": "^0.4.2",
    "loader-utils": "^0.2.12",
    "minifier": "^0.7.1",
    "ng-annotate-loader": "^0.1.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1",
    "sass-loader": "^3.1.2",
    "source-map": "^0.5.3"
  }
}
